	"name": "next",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "bun run db:with-branch -- next dev",
		"build": "next build",
		"start": "next start",
		"lint": "eslint .",
		"test": "vitest",
		"test:e2e": "playwright test",
		"test:e2e:ui": "playwright test --ui",
		"db:migrate": "bun run db:with-branch -- drizzle-kit migrate",
		"db:pull": "drizzle-kit pull",
		"db:reset": "bun run db:with-branch -- bash scripts/reset-db.sh",
		"db:generate": "bun run db:with-branch -- drizzle-kit generate",
		"db:push": "bun run db:with-branch -- drizzle-kit push",
		"db:studio": "bun run db:with-branch -- drizzle-kit studio",
		"db:with-branch": "bun --env-file=.env tsx scripts/with-branch-db.ts",
		"db:template:update": "bun --env-file=.env tsx scripts/update-template-db.ts",
		"db:prod:migrate": "drizzle-kit migrate",
		"db:prod:seed": "tsx src/db/seed.ts",
		"init": "bun install && bun run db:migrate && bun run seed",
		"vercel-build": " (bun run db:prod:migrate || (echo '1st migrate failed; retrying in 8s' && sleep 8 && bun run db:prod:migrate )) && bun run db:prod:seed && next build",
		"biome": "bunx @biomejs/biome check --write .",
		"typecheck": "tsc --noEmit",
		"seed": "bun run db:with-branch -- tsx src/db/seed.ts",
		"mdast-json": "bunx tsx scripts/mdast-json.ts",
		"tipitaka": "bun --env-file=.env scripts/tipitaka-import.ts",
		"translate": "bunx tsx scripts/translate.ts",
		"convert:romn": "bun scripts/convert-romn-to-md.ts"
	},
