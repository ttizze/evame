{
	"name": "next",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "eslint .",
		"test": "vitest",
		"test:e2e": "playwright test",
		"test:e2e:ui": "playwright test --ui",
		"db:migrate": "npx @ariga/atlas schema apply --env local",
		"db:migrate:test": "npx @ariga/atlas schema apply --env test --auto-approve",
		"db:migrate:prod": "npx @ariga/atlas schema apply --env production --auto-approve",
		"db:diff": "atlas schema diff --env local --from \"postgres://postgres:postgres@db.localtest.me:5434/main?search_path=public&sslmode=disable\"  --to 'file://atlas/schema.sql'",
		"db:inspect": "npx @ariga/atlas schema inspect --env local --format '{{ sql .}}' > atlas/schema.sql",
		"db:reset": "bash scripts/reset-db.sh",
		"db:codegen": "bun run kysely-codegen",
		"init": "bun install && bun run db:migrate && bun run seed",
		"atlas:install": "curl -sSf https://atlasgo.sh | sh -s -- -y",
		"vercel-build": "bun run atlas:install && bun run db:diff && bun run seed && next build",
		"biome": "bunx @biomejs/biome check --write .",
		"typecheck": "tsgo -p tsconfig.json --noEmit",
		"seed": "tsx src/db/seed.ts",
		"mdast-json": "bunx tsx scripts/mdast-json.ts",
		"tipitaka": "bun --env-file=.env scripts/tipitaka-import.ts",
		"translate": "bunx tsx scripts/translate.ts",
		"convert:romn": "bun scripts/convert-romn-to-md.ts"
	},
